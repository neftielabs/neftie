{
  "name": "@neftie/common",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "yarn.build": {
    "input": "src",
    "output": "dist"
  },
  "scripts": {
    "build": "tsc",
    "build:prod": "yarn clean && yarn build",
    "dev": "tsc -w",
    "clean": "rimraf -rf ./dist tsconfig.tsbuildinfo",
    "lint": "eslint --ext=ts ./src",
    "format": "prettier --check \"./src/**/*.{js,ts}\"",
    "typecheck": "tsc --project ./tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@types/node": "17.0.21",
    "eslint": "9.2.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "@neftie/prisma": "*",
    "@neftie/subgraph": "*",
    "ethers": "5.6.8",
    "rimraf": "3.0.2",
    "typera-express": "2.4.4",
    "yup": "0.32.11"
  }
}
