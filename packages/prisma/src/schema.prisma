generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String  @id @default(uuid())
  address   String  @unique
  name      String?
  email     String? @unique
  username  String  @unique
  avatarUrl String
  bannerUrl String?
}
