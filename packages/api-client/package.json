{
  "name": "@neftie/api-client",
  "version": "0.0.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "yarn.build": {
    "input": "src",
    "output": "dist"
  },
  "scripts": {
    "build": "tsc",
    "build:prod": "yarn clean && yarn build",
    "dev": "tsc -w",
    "clean": "rimraf -rf ./dist tsconfig.tsbuildinfo",
    "lint": "eslint --ext=ts ./src",
    "format": "prettier --check \"./src/**/*.{js,ts}\"",
    "typecheck": "tsc --project ./tsconfig.json --noEmit"
  },
  "dependencies": {
    "@neftie/common": "*",
    "axios": "0.26.1",
    "form-data": "4.0.0",
    "isomorphic-ws": "4.0.1",
    "reconnecting-websocket": "4.4.0",
    "rimraf": "3.0.2",
    "uuid": "8.3.2",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@types/node": "17.0.21",
    "@types/uuid": "8.3.4",
    "eslint": "9.13.0",
    "typescript": "4.7.4"
  }
}
