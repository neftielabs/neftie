{
  "name": "@neftie/backend",
  "private": true,
  "version": "0.0.0",
  "main": "dist/app.js",
  "yarn.build": {
    "input": "src",
    "output": "dist"
  },
  "scripts": {
    "dev": "yarn build && TS_NODE_BASEURL=./dist tsc-watch -b --onSuccess 'nodemon --exec node -r tsconfig-paths/register ./dist/app.js' --noClear",
    "build": "tsc",
    "env:sync": "env2ts --in .env --out ./src/config/constants.ts"
  },
  "dependencies": {
    "@godaddy/terminus": "4.10.2",
    "@neftie/common": "0.0.0",
    "@neftie/prisma": "0.0.0",
    "@sentry/node": "6.19.2",
    "@sentry/tracing": "6.19.2",
    "axios": "0.26.1",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.0.0",
    "express": "4.17.3",
    "helmet": "5.0.2",
    "hpp": "0.2.3",
    "http-status": "1.5.0",
    "jsonwebtoken": "8.5.1",
    "morgan": "1.10.0",
    "rate-limiter-flexible": "2.3.6",
    "siwe": "1.1.2",
    "typera-express": "2.4.4",
    "winston": "3.6.0",
    "winston-daily-rotate-file": "4.6.1",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@dotenv/cli": "2.2.1",
    "@types/compression": "1.7.2",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/hpp": "0.2.2",
    "@types/jsonwebtoken": "8.5.8",
    "@types/morgan": "1.9.3",
    "@types/node": "17.0.21",
    "copyfiles": "2.4.1",
    "env2ts": "1.4.0",
    "eslint": "8.12.0",
    "nodemon": "2.0.15",
    "rimraf": "3.0.2",
    "tsc-watch": "4.6.2",
    "tsconfig-paths": "3.14.1",
    "typescript": "4.6.3"
  }
}
